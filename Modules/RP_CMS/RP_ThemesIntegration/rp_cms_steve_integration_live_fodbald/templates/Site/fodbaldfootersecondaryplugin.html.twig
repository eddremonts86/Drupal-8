	<section class="SiteFooterSecondary">
		<div class="container">
			<div class="row toTop">
				<div class="col col-md-12">
					<a href="#">
						<i class="fa fa-chevron-up" aria-hidden="true"></i>
					</a>
				</div>
			</div>
			{% if front %}
				<div class="row footer-contact">
					<div class="col col-md-12">
						<h2 class="heading">{{"live_fodbald_footer_text_title" | t}}</h2>
						<p>{{ "live_fodbald_footer_text_1" | t}}</p>
						<p>{{ "live_fodbald_footer_text_2" | t}}</p>
						<p>{{ "live_fodbald_footer_text_3" | t}}</p>
						<p>{{ "live_fodbald_footer_text_4" | t}}</p>
						<div class="row contact">
							<div class="col col-md-6">
								<h2 class="heading">
									<i class="fa fa-map-marker" aria-hidden="true"></i>
									<span>{{"live_fodbald_footer_addr" | t}}</span>
								</h2>
								<p>{{"live_fodbald_footer_addr_text" | t}}</p>
							</div>
							<div class="col col-md-6">
								<h2 class="heading">
									<i class="fa fa-paper-plane" aria-hidden="true"></i>
									<span>{{"live_fodbald_footer_email" | t}}</span>
								</h2>
								<a href="#">{{ "site_info_mail" |t }}</a>
							</div>
						</div>
					</div>
				</div>
			{% endif %}
			<div class="row socials">
				<div class="col col-md-12">
					<ul>
						<li class="fb">
							<a href="#">
								<i class="fa fa-facebook" aria-hidden="true"></i>
							</a>
						</li>
						<li class="twitter">
							<a href="#">
								<i class="fa fa-twitter" aria-hidden="true"></i>
							</a>
						</li>
						<li class="googlp">
							<a href="#">
								<i class="fa fa-google-plus" aria-hidden="true"></i>
							</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</section>