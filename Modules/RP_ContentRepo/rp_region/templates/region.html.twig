{#
/**
 * @file region.html.twig
 * Default theme implementation to present Region data.
 *
 * This template is used when viewing Region pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_region()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('region') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
