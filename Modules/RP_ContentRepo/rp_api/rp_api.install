<?php

/**
 * @file
 * Install, update and uninstall functions for the RP API module.
 */

/**
 * Implements hook_install().
 *
 * @DCG: Optional.
 */
function rp_api_install() {
  drupal_set_message('RP API module has been installed.');
}

/**
 * Implements hook_uninstall().
 *
 * @DCG: Optional.
 */
function rp_api_uninstall() {
  drupal_set_message('RP API module has been uninstalled.');
}
