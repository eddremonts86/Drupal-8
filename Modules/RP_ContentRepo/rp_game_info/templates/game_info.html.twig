{#
/**
 * @file game_info.html.twig
 * Default theme implementation to present Game info data.
 *
 * This template is used when viewing Game info pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_game_info()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('game_info') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
