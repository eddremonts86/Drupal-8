<?php

/**
 * @file
 * Contains rp_game_info.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function rp_game_info_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the rp_game_info module.
    case 'help.page.rp_game_info':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('RP Game Info Entity') . '</p>';
      return $output;

    default:
  }
}
