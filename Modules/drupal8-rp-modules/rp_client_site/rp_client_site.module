<?php

/**
 * @file
 * Contains rp_client_site.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function rp_client_site_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the rp_client_site module.
    case 'help.page.rp_client_site':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function rp_client_site_theme() {
  return [
    'rp_client_site' => [
      'render element' => 'children',
    ],
  ];
}
